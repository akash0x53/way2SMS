	function sendInvite(obj,type){
		if(type=='import'){
			inviteEmails ="";
			var impCont=document.getElementsByName("impCont");
			var len=impCont.length;
			for(var i=0;i<len;i++){
				if(impCont[i].checked){
					inviteEmails +=","+impCont[i].value;
				}
			}
			if(inviteEmails.charAt(0)==',')
				inviteEmails=inviteEmails.substring(1);
			if(inviteEmails.length==0){
				alert("Please select atleast one email id");
				return false;
			}
		}
		else if(type=='manual'){
			inviteEmails = document.getElementById('inviteEmails').value;
			if(inviteEmails==""){
				alert("Please specify at least one recipient");
				document.getElementById('inviteEmails').focus();
				return false;
			}
		}
		var alen = 0; 
		var arrto=inviteEmails.split(",");
		alen = arrto.length;
		for(var j=0;j<alen-1;j++){
			if(arrto[j].indexOf("@") == -1){
				alert("Please enter valid email ids");
				document.getElementById('inviteEmails').focus();
				return false;
			}
		}
		if(arrto[alen-1]==""){
			alen = alen-1;	
		}
		if(emailValidation(inviteEmails)){		
			var paramData = "inviteURL="+document.getElementById('inviteURLTArea').value+"&inviteEmails="+inviteEmails; 
			var url = "invite.action";  
			obj.disabled="disabled";
			obj.value="Please wait.."; 
			var callback = {
				success : function(o){
					var res=o.responseText; 
					if(res=="session out"){
						window.parent.logout1();
					}
					else{
						alert("Invitation's Sent Successfully");
						 close1();
						//window.location="./InviteConfirm.action?r="+res;
					} 
				},
				failure : function(o){  
					alert("Failure :: Something had gone wrong...Try again...");
					obj.disabled="";
					obj.value="Invite";
				},
				argument : [],
				timeout : 300000
			}
			var request = window.parent.YAHOO.util.Connect.asyncRequest('POST', url, callback, paramData);	
		} 
	}
