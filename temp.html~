	function checkemail(str){
		var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i
		if (filter.test(str))
			testresults=true;
		else
			testresults=false;
		return (testresults);
	}
	function ltrim(stringToTrim) {
		return stringToTrim.replace(/^\s+/,"");
	}
	function rtrim(stringToTrim) {
		return stringToTrim.replace(/\s+$/,"");
	}
	function emailValidation(inviteEmails){
		var arrto=inviteEmails.split(",");
		if(arrto.length>1){
			var temparr = new Array();
			var tempemail = new Array();
			for(var i=0;i<=arrto.length-1;i++){
				if(arrto[i]!=""){
					if(arrto[i].indexOf('<')!=-1&&arrto[i].indexOf('>')!=-1){
						temparr=arrto[i].split("<");
						if(temparr.length==2){
							var val1=temparr[0].match(/[^\w]/);
							temparr=temparr[1].split(">");
							if(val1==null&&temparr[1]==""){
								if(temparr.length==2){
									tempemail=temparr[0];
								}else{
									tempemail=arrto[i];
								}
							}else{
								tempemail=arrto[i];
							}
						}else{
							tempemail=arrto[i];
						}			
					}else{
							tempemail=arrto[i];
					}
					tempemail = ltrim(tempemail);
					tempemail = rtrim(tempemail);
					if (checkemail(tempemail)==false){ 
						//alert("Invalid Email Address ("+tempemail+")");
						//return false;
					}
				}
			}
		}
		else{
			var temparr=inviteEmails.split("<");
			if(temparr.length>1){
				temparr=temparr[1].split(">");
				inviteEmails=temparr[0];
			}
			if (checkemail(inviteEmails)==false){ 
				alert("Invalid Email Address ("+inviteEmails+")");
				return false;
			}
		}
		return true;
	}
